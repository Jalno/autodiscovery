{
    "name": "jalno/autodiscovery",
    "description": "Jalno AutoDiscovery",
    "version": "0.0.3",
    "autoload": {
        "psr-4": {
            "Jalno\\AutoDiscovery\\": "src"
        }
    },
    "require": {
        "jalno/lumen": "^0.0.1"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.7.6",
        "orchestra/testbench": "^6.18.0"
    },
    "scripts": {
        "test:types": "./vendor/phpstan/phpstan/phpstan analyse -vvv --debug --memory-limit 256M phpstan.neon src",
        "test": [
            "@test:types"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
